{
  "name": "clash-config",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "$npm_execpath run build && vite preview",
    "deploy": "npm_execpath run build && wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "pb-gen": "pbjs -t static-module -w es6 -o ./src/geo/geo.js ./src/geo/geo.proto",
    "pb-gen-dts": "pbts -o ./src/geo/geo.d.ts ./src/geo/geo.js",
    "cli": "tsx cli.ts",
    "wrangler": "wrangler"
  },
  "dependencies": {
    "@inquirer/prompts": "^7.8.6",
    "@types/yargs": "^17.0.33",
    "hono": "^4.9.9",
    "js-yaml": "^4.1.0",
    "protobufjs": "^7.5.4",
    "yargs": "^18.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.16",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.9.2",
    "prettier": "^3.6.2",
    "protobufjs-cli": "^1.1.3",
    "tsx": "^4.20.6",
    "vite": "^7.1.12",
    "vite-ssr-components": "^0.5.1",
    "wrangler": "^4.4.0"
  }
}
